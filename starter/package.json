    "jscpd": "0.6.15",
    "mocha": "4.0.1",
    "nyc": "11.2.1",
    "rimraf": "2.6.2",
    "semantic-release": "8.2.0",
    "supertest": "3.0.0",
    "tslint": "5.8.0",
